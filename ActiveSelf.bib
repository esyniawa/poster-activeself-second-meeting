@inproceedings{schillaciBodyRepresentationsRobot2016,
  title = {Body {{Representations}} for {{Robot Ego-Noise Modelling}} and {{Prediction}}. {{Towards}} the {{Development}} of a {{Sense}} of {{Agency}} in {{Artificial Agents}}},
