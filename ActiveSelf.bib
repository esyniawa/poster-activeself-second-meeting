@article{dacreCerebellarthalamocorticalPathwayDrives2021,
  title = {A Cerebellar-Thalamocortical Pathway Drives Behavioral Context-Dependent Movement Initiation},
  author = {Dacre, Joshua and Colligan, Matt and Clarke, Thomas and Ammer, Julian J. and Schiemann, Julia and Chamosa-Pino, Victor and Claudi, Federico and Harston, J. Alex and Eleftheriou, Constantinos and Pakan, Janelle M.P. and Huang, Cheng-Chiu and Hantman, Adam W. and Rochefort, Nathalie L. and Duguid, Ian},
  date = {2021-07},
  journaltitle = {Neuron},
  shortjournal = {Neuron},
  volume = {109},
  number = {14},
  pages = {2326-2338.e8},
  issn = {08966273},
  doi = {10.1016/j.neuron.2021.05.016},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0896627321003561},
  urldate = {2023-05-22},
  abstract = {Executing learned motor behaviors often requires the transformation of sensory cues into patterns of motor commands that generate appropriately timed actions. The cerebellum and thalamus are two key areas involved in shaping cortical output and movement, but the contribution of a cerebellar-thalamocortical pathway to voluntary movement initiation remains poorly understood. Here, we investigated how an auditory ‘‘go cue’’ transforms thalamocortical activity patterns and how these changes relate to movement initiation. Population responses in dentate/interpositus-recipient regions of motor thalamus reflect a time-locked increase in activity immediately prior to movement initiation that is temporally uncoupled from the go cue, indicative of a fixed-latency feedforward motor timing signal. Blocking cerebellar or motor thalamic output suppresses movement initiation, while stimulation triggers movements in a behavioral context-dependent manner. Our findings show how cerebellar output, via the thalamus, shapes cortical activity patterns necessary for learned context-dependent movement initiation.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/active self/Cerebellum/Thalamic Pathway/1-s2.0-S0896627321003561-main.pdf}
}

@article{dangeloSenseAgencyShapes2018,
  title = {The Sense of Agency Shapes Body Schema and Peripersonal Space},
  author = {D’Angelo, Mariano and family=Pellegrino, given=Giuseppe, prefix=di, useprefix=true and Seriani, Stefano and Gallina, Paolo and Frassinetti, Francesca},
  date = {2018-09-14},
