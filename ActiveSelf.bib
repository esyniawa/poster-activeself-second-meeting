@book{10.7551/mitpress/12441.001.0001,
  title = {Active Inference: {{The}} Free Energy Principle in Mind, Brain, and Behavior},
  author = {Parr, Thomas and Pezzulo, Giovanni and Friston, Karl J.},
  date = {2022-03},
  publisher = {{The MIT Press}},
  doi = {10.7551/mitpress/12441.001.0001},
  url = {https://doi.org/10.7551/mitpress/12441.001.0001},
  abstract = {The first comprehensive treatment of active inference, an integrative perspective on brain, cognition, and behavior used across multiple disciplines. Active inference is a way of understanding sentient behavior—a theory that characterizes perception, planning, and action in terms of probabilistic inference. Developed by theoretical neuroscientist Karl Friston over years of groundbreaking research, active inference provides an integrated perspective on brain, cognition, and behavior that is increasingly used across multiple disciplines including neuroscience, psychology, and philosophy. Active inference puts the action into perception. This book offers the first comprehensive treatment of active inference, covering theory, applications, and cognitive domains. Active inference is a “first principles” approach to understanding behavior and the brain, framed in terms of a single imperative to minimize free energy. The book emphasizes the implications of the free energy principle for understanding how the brain works. It first introduces active inference both conceptually and formally, contextualizing it within current theories of cognition. It then provides specific examples of computational models that use active inference to explain such cognitive phenomena as perception, attention, memory, and planning.},
  isbn = {978-0-262-36997-8},
  file = {/home/erik/Nextcloud/Literature/active self/Books/active inference/1_Overview.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/2_Roadto.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/3_HighRoad.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/4_GenerativeModels.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/5_Neurobiology.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/6_Design.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/7_DiscreteTime.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/8_ContinueousTime.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/9_DataAnalysis.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/App_A_Math.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/App_B_Eq.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/App_C_matlabCode.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/App_D_Notes.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/Index.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/References.pdf;/home/erik/Nextcloud/Literature/active self/Books/active inference/Title_ToC.pdf}
}

@article{andersenMULTIMODALREPRESENTATIONSPACE1997,
  title = {{{MULTIMODAL REPRESENTATION OF SPACE IN THE POSTERIOR PARIETAL CORTEX AND ITS USE IN PLANNING MOVEMENTS}}},
  author = {Andersen, Richard A. and Snyder, Lawrence H. and Bradley, David C. and Xing, Jing},
  date = {1997-03},
  journaltitle = {Annual Review of Neuroscience},
  shortjournal = {Annu. Rev. Neurosci.},
  volume = {20},
  number = {1},
  pages = {303--330},
  issn = {0147-006X, 1545-4126},
  doi = {10.1146/annurev.neuro.20.1.303},
  url = {https://www.annualreviews.org/doi/10.1146/annurev.neuro.20.1.303},
  urldate = {2023-09-21},
  abstract = {Recent experiments are reviewed that indicate that sensory signals from many modalities, as well as efference copy signals from motor structures, converge in the posterior parietal cortex in order to code the spatial locations of goals for movement. These signals are combined using a specific gain mechanism that enables the different coordinate frames of the various input signals to be combined into common, distributed spatial representations. These distributed representations can be used to convert the sensory locations of stimuli into the appropriate motor coordinates required for making directed movements. Within these spatial representations of the posterior parietal cortex are neural activities related to higher cognitive functions, including attention. We review recent studies showing that the encoding of intentions to make movements is also among the cognitive functions of this area.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/active self/2read/gain modulation/andersen (1997) multimodal representation of space in the posterior parietal cortex and its use in planning movements.pdf}
}

@article{anWholeBrainConnectivityMap2021,
  title = {A {{Whole-Brain Connectivity Map}} of {{VTA}} and {{SNc Glutamatergic}} and {{GABAergic Neurons}} in {{Mice}}},
  author = {An, Sile and Li, Xiangning and Deng, Lei and Zhao, Peilin and Ding, Zhangheng and Han, Yutong and Luo, Yue and Liu, Xin and Li, Anan and Luo, Qingming and Feng, Zhao and Gong, Hui},
  date = {2021-12-23},
  journaltitle = {Frontiers in Neuroanatomy},
  shortjournal = {Front. Neuroanat.},
  volume = {15},
  pages = {818242},
  issn = {1662-5129},
  doi = {10.3389/fnana.2021.818242},
  url = {https://www.frontiersin.org/articles/10.3389/fnana.2021.818242/full},
  urldate = {2023-05-22},
  abstract = {The glutamatergic and GABAergic neurons in the ventral tegmental area (VTA) and substantia nigra pars compacta (SNc) mediated diverse brain functions. However, their whole-brain neural connectivity has not been comprehensively mapped. Here we used the virus tracers to characterize the whole-brain inputs and outputs of glutamatergic and GABAergic neurons in VTA and SNc. We found that these neurons received similar inputs from upstream brain regions, but some quantitative differences were also observed. Neocortex and dorsal striatum provided a greater share of input to VTA glutamatergic neurons. Periaqueductal gray and lateral hypothalamic area preferentially innervated VTA GABAergic neurons. Specifically, superior colliculus provided the largest input to SNc glutamatergic neurons. Compared to input patterns, the output patterns of glutamatergic and GABAergic neurons in the VTA and SNc showed significant preference to different brain regions. Our results laid the anatomical foundation for understanding the functions of cell-type-specific neurons in VTA and SNc.},
  langid = {english},
  file = {/home/erik/Nextcloud/Literature/active self/Cerebellum/SNc/fnana-15-818242.pdf}
}

