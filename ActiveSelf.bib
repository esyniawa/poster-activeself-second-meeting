@article{miquelCerebellumCocaine2020,
  title = {The {{Cerebellum}} on {{Cocaine}}},
  author = {Miquel, Marta and Gil-Miravet, Isis and Guarque-Chabrera, Julian},
  date = {2020-10-21},
  journaltitle = {Frontiers in Systems Neuroscience},
