@article{insolaReductionAmplitudeSubcortical2004,
  title = {Reduction in Amplitude of the Subcortical Low- and High-Frequency Somatosensory Evoked Potentials during Voluntary Movement: An Intracerebral Recording Study},
  shorttitle = {Reduction in Amplitude of the Subcortical Low- and High-Frequency Somatosensory Evoked Potentials during Voluntary Movement},
  author = {Insola, Angelo and Le Pera, Domenica and Restuccia, Domenico and Mazzone, Paolo and Valeriani, Massimiliano},
